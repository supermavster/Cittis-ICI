<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FinishSave">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView5" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="216dp"
            android:layout_height="216dp" app:srcCompat="@mipmap/ic_launcher_round"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/imageView3"
            app:layout_constraintHorizontal_bias="0.497" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:scaleType="fitXY"
            tools:ignore="VectorDrawableCompat"/>
    <Button
            android:text="@string/save"
            android:layout_width="263dp"
            android:layout_height="91dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/btn_save_all" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"/>


</android.support.constraint.ConstraintLayout>